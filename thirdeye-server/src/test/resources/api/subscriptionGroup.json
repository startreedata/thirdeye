[
  {
    "name": "my-sg",
    "cron": "*/1 * * * * ?",
    "alerts": [
      {
        "id": 15263
      }
    ],
    "notificationSchemes": {
      "email": {
        "to": [
          "pyne.suvodeep@gmail.com"
        ]
      }
    }
  },
  {
    "name": "sg-with-spec",
    "cron": "*/1 * * * * ?",
    "specs": [
      {
        "type": "email-smtp",
        "params": {
          "smtp": {
            "host": "${SMTP_HOST}",
            "port": "${SMTP_PORT}",
            "user": "${SMTP_USER}",
            "password": "${SMTP_PASSWORD}"
          },
          "emailRecipients": {
            "from": "thirdeye-alerts@startree.ai",
            "to": [
              "suvodeep@startree.ai"
            ]
          }
        }
      }
    ]
  },
  {
    "name": "email-sendgrid",
    "cron": "*/1 * * * * ?",
    "specs": [
      {
        "type": "email-sendgrid",
        "params": {
          "apiKey": "${SENDGRID_API_KEY}",
          "emailRecipients": {
            "from": "thirdeye-alerts@startree.ai",
            "to": [
              "suvodeep@startree.ai"
            ]
          }
        }
      }
    ]
  },
  {
    "name": "slack-webhook",
    "cron": "*/1 * * * * ?",
    "specs": [
      {
        "type": "slack",
        "params": {
          "webhookUrl": "${SLACK_WEBHOOK_URL}"
        }
      }
    ]
  },
  {
    "name": "webhook",
    "cron": "*/1 * * * * ?",
    "specs": [
      {
        "type": "webhook",
        "params": {
          "url": "http://localhost:8080/internal/webhook"
        }
      }
    ]
  }
]
