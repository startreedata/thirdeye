{
  "name": "HoltWinters with template startree-holt-winters",
  "description": "Uses template startree-holt-winters.",
  "template": {
    "name": "startree-holt-winters"
  },
  "templateProperties": {
    "dataSource": "pinotQuickStartLocal",
    "dataset": "pageviews",
    "timeColumn": "date",
    "timeColumnFormat": "yyyyMMdd",
    "aggregationFunction": "sum",
    "aggregationColumn": "views",
    "lookback": "P14D",
    "monitoringGranularity": "P1D",
    "completenessDelay": "P0D",
    "pattern": "UP_OR_DOWN",
    "sensitivity": "1",
    "holtWintersLookback": "14"
  },
  "cron": "0 0 0 1/1 * ? *"
}
