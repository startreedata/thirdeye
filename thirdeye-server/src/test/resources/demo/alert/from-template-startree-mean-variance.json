{
  "name": "Mean-Variance with template startree-mean-variance",
  "description": "Uses template startree-mean-variance.",
  "template": {
    "name": "startree-mean-variance"
  },
  "templateProperties": {
    "dataSource": "pinotQuickStartLocal",
    "dataset": "pageviews",
    "timeColumn": "date",
    "timeColumnFormat": "yyyyMMdd",
    "aggregationFunction": "sum",
    "aggregationColumn": "views",
    "lookback": "P14D",
    "monitoringGranularity": "P1D",
    "completenessDelay": "P0D",
    "pattern": "UP_OR_DOWN",
    "sensitivity": "1"
  },
  "cron": "0 0 0 1/1 * ? *"
}
