    "description": "UI project for StarTree ThirdEye",
    "keywords": [
        "startree",
        "star-tree",
        "star tree",
        "ui",
        "startreeui",
        "startree-ui",
        "startree ui",
        "thirdeye",
        "thirdeye-ui"
    ],
    "author": {
        "name": "StarTree",
        "url": "https://www.startree.ai"
    },
    "license": "UNLICENSED",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/startreedata/thirdeye.git",
        "directory": "thirdeye-ui"
    },
    "main": "dist/index.html",
    "types": "",
    "files": [
        "dist"
    ],
    "scripts": {
        "prepare": "cd .. && husky install thirdeye-ui/.husky",
        "precommit": "lint-staged --concurrent false",
        "start": "webpack serve --config ./webpack.config.dev.js --env development",
        "build": "webpack --config ./webpack.config.prod.js --env production",
        "test": "jest --passWithNoTests",
        "test-watch": "jest --passWithNoTests --watch",
        "test-coverage": "jest --passWithNoTests --coverage",
        "test-e2e": "node_modules/.bin/cypress run --headless --config-file cypress.config.json",
        "test-e2e-gui": "node_modules/.bin/cypress open --config-file cypress.config.json",
        "license-header-check": "license-check-and-add check --config-file .licenseheaderrc.json",
        "license-header-fix": "license-check-and-add add --config-file .licenseheaderrc.json --regex-replacements $(date +%Y)",
        "eslint-check": "eslint '**/*.{ts,tsx,js,json}' --no-error-on-unmatched-pattern",
        "eslint-fix": "eslint '**/*.{ts,tsx,js,json}' --no-error-on-unmatched-pattern --fix",
        "stylelint-check": "stylelint '**/*.{css,scss}' --allow-empty-input",
        "stylelint-fix": "stylelint '**/*.{css,scss}' --allow-empty-input --fix",
        "prettier-check": "prettier . --check",
        "prettier-fix": "prettier . --write",
        "lint-check": "npm run license-header-check && npm run eslint-check && npm run stylelint-check && npm run prettier-check",
        "lint-fix": "npm run license-header-fix && npm run eslint-fix && npm run stylelint-fix && npm run prettier-fix",
        "release": "semantic-release"
    },
